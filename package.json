{
  "__SECTION_GULP": "",
  "fullName": "$dom.component",
  "version": "1.2.0",
  "build": "0001",
  "src_folder": "src/",
  "bin_folder": "bin/",
  "standalone": "standalone",
  "__Comment__sequence": "exclamation mark (!) on the start of task name means to escape it",
  "sequence": [
    "javascript",
    "doc",
    "examples"
  ],
  "__SECTION_NPM": "",
  "name": "dollar_dom_component",
  "description": "JavaScript Utils",
  "main": "gulpfile.js",
  "bin": {
    "jaajsu": "$dom_component-min.js"
  },
  "directories": {
    "example": "example",
    "bin": "bin"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "jshint": "node ./node_modules/jshint/bin/jshint --verbose",
    "jshint_full": "node ./node_modules/jshint/bin/jshint --verbose --show-non-errors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaandrle/dollar_dom_component.git"
  },
  "author": "Jan Andrle <andrle.jan@centrum.cz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaandrle/dollar_dom_component/issues"
  },
  "homepage": "https://github.com/jaandrle/dollar_dom_component",
  "dependencies": {
    "gulp": ">3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-javascript-obfuscator": "1.1.5",
    "gulp-minify": "3.1.0",
    "gulp-place": "^1.8.0",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-util": "3.0.8",
    "jshint": "^2.12.0"
  },
  "__SECTION__JSHINT": "",
  "jshintConfig": {
    "esversion": 6,
    "browser": "true",
    "undef": "true",
    "latedef": "true",
    "maxparams": 3,
    "maxdepth": 2,
    "maxcomplexity": 11
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^5.0.1"
  }
}
